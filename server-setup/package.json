  "build": {
    "appId": "com.azadipos.serversetup",
    "productName": "AzadiPOS Server Setup",
    "win": {
      "target": ["nsis", "portable"]
    },
    "extraResources": [
      {
        "from": "../prisma",
        "to": "prisma"
      }
    ]
  }
}
